Gauss prueba_factorial_recursivo

int factorial(int num) {
    if ( num == 0 ) {
        return 1
    }
    else {
        return num * factorial(num - 1)
    }
}

main() {
    var int x, y
    x = factorial(0)
    print(x)
    print(factorial(1))
    print(factorial(2))
    print(factorial(3))
    print(factorial(4))
    print(factorial(5))
    print(factorial(6))
    print(factorial(7))
}